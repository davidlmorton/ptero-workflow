lsf: cd $PTERO_LSF_HOME; tox -r -e dev -- --num-workers=$PTERO_WORKFLOW_TEST_NUM_LSF_WORKERS --procfile=Procfile
petri: cd $PTERO_PETRI_HOME; tox -r -e dev -- --procfile=Procfile
web: coverage run ptero_workflow/api/wsgi.py
http_worker: coverage run $(which celery) worker -n workflow_http_worker.%h.$PORT -A ptero_workflow.implementation.celery_app -Q http
worker: coverage run $(which celery) worker -n workflow_submit_worker.%h.$PORT -A ptero_workflow.implementation.celery_app -Q submit
