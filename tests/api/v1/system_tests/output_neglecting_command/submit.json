{
    "tasks": {
        "A": {
            "methods": [
                {
                    "name": "execute",
                    "service": "shell-command",
                    "parameters": {
                        "commandLine": ["./output_neglecting_command"],
                        "user": "{{ user }}",
                        "workingDirectory": "{{ workingDirectory }}",
                        "environment": {{ environment }}
                    }
                }
            ]
        }
    },

    "links": [
        {
            "source": "input connector",
            "destination": "A",
            "dataFlow": {
                "in_a": "param_a",
                "in_b": "param_b"
            }
        },
        {
            "source": "A",
            "destination": "output connector",
            "dataFlow": {
                "param_a": "out_a",
                "param_b": "out_b"
            }
        }
    ],

    "inputs": {
        "in_a": "kittens",
        "in_b": "puppies"
    }
}
